public String evensOnly(String original) {
	if (original.equals(null)) {
		return null;
	}
	else if (original.equals("")) {
		return "String is empty.";
	}
	else {
		String nuevo;
		for (int i = 0; i < original.length(); i++) {
			if (original.index(i) % 2 == 0) {
				nuevo = nuevo + original.charAt(i);
			}
		}
		return nuevo;
	}
